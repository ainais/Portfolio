package erp;

public class Employee {

	public int EmployeeID;
	public String Name;
	public String Department;
	public String EnteringDate;
	public int RemainVac;
	
	public Employee() {
	}

	public Employee(int employeeID, String name, String department, String enteringDate, int remainVac) {
		EmployeeID = employeeID;
		Name = name;
		Department = department;
		EnteringDate = enteringDate;
		RemainVac = remainVac;
	}
	

}
