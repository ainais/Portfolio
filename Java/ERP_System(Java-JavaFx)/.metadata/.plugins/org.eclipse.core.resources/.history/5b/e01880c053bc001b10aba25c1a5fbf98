package erp;

public class InfoEmployee {

	public int EmployeeID;
	public String Name;
	public String Department;
	public String EnteringDate;
	public int RemainVac;
	
	public InfoEmployee() {
	}

	public InfoEmployee(int employeeID, String name, String department, String enteringDate, int remainVac) {
		EmployeeID = employeeID;
		Name = name;
		Department = department;
		EnteringDate = enteringDate;
		RemainVac = remainVac;
	}

	public int getEmployeeID() {
		return EmployeeID;
	}

	public void setEmployeeID(int employeeID) {
		EmployeeID = employeeID;
	}

	public String getName() {
		return Name;
	}

	public void setName(String name) {
		Name = name;
	}

	public String getDepartment() {
		return Department;
	}

	public void setDepartment(String department) {
		Department = department;
	}

	public String getEnteringDate() {
		return EnteringDate;
	}

	public void setEnteringDate(String enteringDate) {
		EnteringDate = enteringDate;
	}

	public int getRemainVac() {
		return RemainVac;
	}

	public void setRemainVac(int remainVac) {
		RemainVac = remainVac;
	}
	
	
	

}
